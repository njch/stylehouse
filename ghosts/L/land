
name: trail of ways...

include: W/tractor
tractors:
  - K: Thing
    k: ''
    arr_if: |
      1
    B_: |
      $ar->{name} || $ar->{code} || $thing
    print: |
      $B->{name} || $B->{thing};
    to_travel_this: |
      die "TRAVELLING THIS!";
      $B->{code} ? $B->{code}->() : $B->{thing};

