
name: trail of ways...

include: W/tractor
tractors:
  - K: Thing
    k: ''
    arr_if: |
      1
    B_: |
      $ar->{name} || $ar->{code} || $thing
    print: |
      $B->{name} || $B->{thing};
    to_travel_this: |
      $G->timer(0.001, sub {
        $B->{code}->();
      }, "L/funcencode");
        

