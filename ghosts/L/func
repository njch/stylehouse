
name: trail of ways...
tractors:
  - K: Thing
    arr_if: |
      1
    B_: |
      $ar->{name} || $ar->{code} || $thing
    print: |
      $B->{name} || $B->{thing};
    to_travel_this: |
      Info "CarefuL", $S;
      $B->{code} ? $B->{code}->() : $B->{thing};
hooks:
  recoded_init: |
    my $thing = G(tractor)->w('random');
    $O->w('somewhere', { thing => $thing } );
  arr: |
    my $S = Arr[(%$ar)];

