
name: trail of ways...
tractors:
  - K: Thing
    k: ''
    arr_if: |
      1
    B_: |
      $ar->{name} || $ar->{code} || $thing
    print: |
      $B->{name} || $B->{thing};
    to_travel_this: |
      $G->timer(0.001, sub {
        $B->{code}->();
      }, "L/funcencode");
        
hooks:
  recoded_init: |
    my $thing = G(tractor)->w('random');
    $O->w('somewhere', { thing => $thing } );
  arr: |
    my $S = Arr[$name, $code];

