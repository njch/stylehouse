ooj/flow: |
   my $A = {J=>$J,d=>$d};
   
   # 3
   
   # 7
   
   # 9
   d.s
ooj/Y:
    -
     - cts_Y #
     - 2
     - |
       for my $ct (@{wJ.cts.Y}) {
           next unless ct.any or exists $d->{ct.t_k} && exists $d->{ct.t_k}->{ct.t};
           my $v = $d->{ct.t_k}->{ct.t} unless ct.any;
           my $at = {};
           my $T = {};
           my $C = $d; # bendy stick throw
           my $c = C.c; # <v contation
           my $g = wJ.ctx.Y;
           
           wJ.ctx.Y->{ct.x}->($A,$C,$g,$T,$J,$d,$v,$at);
           
           return die @{T.return} if T.return;
           die "atgot ". ki $at if %$at;
           die "Tgot ". ki $T if %$T;
       }
       
ooj/C:
    -
     - cts_C #
     - 5
     - |
       A.note = {};
       A.note.not = sub {
           my ($ct,$k) = @_;
           A.note.>$ct.>$k = 1;
       };
       
       my $g = wJ.ctx.C;
       for my $ct (@{wJ.cts.C}) {
           next unless ct.any or exists $d->{ct.t_k}->{ct.t};
           my $v = $d->{ct.t_k}->{ct.t} unless ct.any;
           ct.t_k eq "c" && A.note.not&c,ct.t;
           my $at = {};
           my $T = {};
           my $C = $d; # bendy stick throw
           my $c = C.c; # <v contation
           
           wJ.ctx.C->{ct.x}->($A,$C,$g,$T,$J,$d,$v,$at);
           
           
           if (%$at) {
               sayyl "atgot ". ki $at;
               Rw jamsc $J $d $sc:at;
           }
           return @{T.return} if T.return;
           last if T.last;
           die "Tgot ". ki $T if %$T;
       }
       
ooj/ing: 
   - MZ: Cc:Y
   -
    - sc
    - 1
    - |
     d.sc = delete d.c.sc;
      
   - MZ: Cc:Cany
   -
    - gets
    - 2
    - |
      d.s = A.jr->();
      # from a way
      # then somewhere just way/J.name 
      A.comp = $d;
   -
    - puts
    - 7
    - |
       my @jh = grep{!A.note.c->{$_}}keys %{d.c};
       if (@jh) {
           sayyl "oo  d.t d.cv   ->  ".join "      ",
               map { "$_ = ".gp d.c->{$_}}@jh;
           A.hav = 1;
       }
       else {
           sayyl "oo  d.t d.cv";
       }
       # some alt ^ make round v
       # 
       die if !d.t;
       die if !d.y.cv;
       die if !ref d.s;
       
       J.bz->("d.t\t".d.y.cv,d.s);
ooj/JJ: 
   -
    - jr
    - 3
    - |
      A.jr = sub {
         my $s = shift;
         my $v = shift;  
         $s = '%'.$s if $s;  
         my $r = "d.t$s\t".d.y.cv;
         !defined $v ? J.bb->{$r} :
         do { J.bz->($r, $v);
           sub { delete J.bb->{$r} } 
         };
      };
   - MZ: Cc:Y
   -
    - J
    - 3
    - |
     d.t = ref $v ? v.name : $v;
     d.y.cv ||= 0.3;
     
     
   - MZ: Cc:C
   -
    - J
    - 1
    - |
       die "tis" if ref d.c.J;
   -
    - J
    - 3
    - |
       if (!ref d.s) {
           my $o = {name=>d.c.J};
           d.s = Rw JJ $o;
           sayyl "oo Made o.name";
       }
       die if !ref d.s;

   - MZ: Cc:Cany
   -
    - Joing 
