ooj/JJ: 
   - MZ: Cc:Y
   -
    - J
    - 3
    - |
     d.t = ref $v ? v.name : $v;
     d.cv = 0.3;
     
   - MZ: Cc:C
   -
    - J
    - 3
    - |
       die "tis" if ref d.c.J;
       if (!ref d.s) {
           my $o = {name=>d.c.J};
           d.s = Rw JJ $o;
           sayyl "oo Made o.name";
       }
       die if !ref d.s;

   - MZ: Cc:Dany
   -
    - Joing 
    - 3
    - |
       my $ar = {%{d.c}};
       ar.J=d.s;
       
       return d.s if !A.hav && A.jr&ok-9;
       my $n = A.hav ? sub{} : A.jr&ok-9,9;
       Rw around;
       $n->();
ooj/time:
   - MZ: Cc:Yany
   -
    - ok
    - 3
    - |
      A.ztime = d.c.time || 6;
      A.time = sub {
           my ($J,$wa) = @_;
           warn "$J, $wa";
           A.ztime >= $wa ? 1 : do {
               A.fopi->{J.id} = $J;
               0;
           };
      };
      
