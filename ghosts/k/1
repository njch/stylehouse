ooj/ing: 
   - MZ: Cc:Y
   -
    - sc
    - 1
    - |
     d.sc = delete d.c.sc;
      
   - MZ: Cc:Yany
   -
    - jr
    - 4
    - |
     A.jr = sub {
         my $s = shift;
         my $v = shift;  
         $s = '%'.$s if $s;  
         my $r = "d.t$s\td.cv";
         !defined $v ? J.bb->{$r} :
         do { J.bz->($r, $v);
           sub { delete J.bb->{$r} } 
         };
     };
     sayyl 'jring  '. wdump 3, $d;
      
   - MZ: Cc:Cany
   -
    - gets
    - 2 
    - |
      d.s = A.jr->();
   -
    - puts
