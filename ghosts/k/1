oo/flows/ooj: |
   flow 1
   Y 2
   S 5
   ing 3
   file 3
   
ooj/flow: |
   my $A = {J=>$J,d=>$d};
   
   # 3
   sayyl ki 1, $d; 
   
   A.jr = sub {
       my $s = shift;
       my $v = shift;  
       $s = '%'.$s if $s;  
       my $r = "d.t$s\td.cv";
       !defined $v ? J.bb->{$r} :
       do { J.bb->{$r} = $v;
         sub { delete J.bb->{$r} } 
       };
   };
   
   # 5
   
   # 7
   my $ig = {map{$_,1}qw'file J ok'};
   my @jh = grep{!$ig->{$_}}keys %{d.c};
   warn "Have: @jh" if @jh;
   my $hav = 1 if @jh;
   # some alt ^ make round v
   # 
   die if !d.t;
   die if !d.cv;
   die if !ref d.s;
   
   J.bz->("d.t\td.cv",d.s);
   
   do {
       my $ar = {%{d.c}};
       ar.J=d.s;
       
       return d.s if !$hav && A.jr&ok-9;
       my $n = $hav ? sub{} : A.jr&ok-9,9;
       Rw around;
       $n->();
   };
   d.s
ooj/Y:
    -
     - cts_Y #
     - 2
     - |
       for my $ct (@{wJ.cts.Y}) {
           next unless ct.any or exists $d->{ct.t_k}->{ct.t};
           my $v = $d->{ct.t_k}->{ct.t} unless ct.any;
           my $at = {};
           my $T = {};
           my $C = $d; # bendy stick throw
           my $c = C.c; # <v contation
           my $g = wJ.ctx.Y;
           
           wJ.ctx.Y->{ct.x}->($A,$J,$C,$c,$d,$v,$g,$at,$T);
           
           return die @{T.return} if T.return;
           die "atgot ". ki $at if %$at;
           die "Tgot ". ki $T if %$T;
       }
       
ooj/S:
    -
     - cts_C #
     - 5
     - |
       my $g = wJ.ctx.C;
       for my $ct (@{wJ.cts.C}) {
           next unless ct.any or exists $d->{ct.t_k}->{ct.t};
           my $v = $d->{ct.t_k}->{ct.t} unless ct.any;
           my $at = {};
           my $T = {};
           my $C = $d; # bendy stick throw
           my $c = C.c; # <v contation
           
           wJ.ctx.C->{ct.x}->($A,$J,$C,$c,$d,$v,$g,$at,$T);
           
           return @{T.return} if T.return;
           die "atgot ". ki $at if %$at;
           die "Tgot ". ki $T if %$T;
       }
       
ooj/ing: 
   - MZ: Cc:Y
   -
    - sc
    - 1
    - |
     d.sc = delete d.c.sc;
   -
    - J
    - 2
    - |
     d.t = ref $v ? v.name : $v;
     d.cv = 0.3;
      
   - MZ: Cc:Cany
   -
    - gets
    - 2
    - |
      d.s = A.jr->();
      
   - MZ: Cc:C
   -
    - J
    - 3
    - |
       die "tis" if ref d.c.J;
       if (!d.s) {
           my $o = {name=>d.c.J};
           d.s = Rw JJ $o;
           sayyl "oo Made o.name";
       }
       die if !ref d.s;
   -
    - ok
    - 3.3
    - |
       my $ok = d.sc.ok;
       my $co = A.jr&ok-$ok;
       saygr "OK: ".ki 2, $d; 
       #$ok && hitime- 
       T.return = [d.s];
ooj/file:
   - MZ: Cc:Y
   -
    - file
    - 2
    - |
     d.t = $v;
     d.cv = 0.4;
   - MZ: Cc:C
   -
    - file
    - 3
    - |
      sayyl "YEAH! $v ist d.s";
       return if d.s;
       
       T.return = [];
 

