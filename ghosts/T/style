name: Styling
chains:
 - K: 'ARRAY'
   arr_if: |
   $a->{K} eq ref $thing eq 'ARRAY'
   foreach_travel:
     each: @$thing
     next_thing: $each
   print: |
        $S->{i}

 - K: 'Wormhole'
   arr_if: |
   $a->{K} eq ref $thing eq 'ARRAY'
   foreach_travel:
     each: @$thing
     next_thing: $each
   print: |
        $S->{i}

 - K: 'Wormhole'
   arr_if: |
   $a->{K} eq ref $thing eq 'ARRAY'
   foreach_travel:
     each: @$thing
     next_thing: $each
   print: |
        $S->{i}

 - K: 'HASH'#c
   arr_if: |
     $a->{K} eq "Wormhole" && ref $thing eq 'HASH' && $thing->{
   foreach_travel:
     each: sort keys %$thing
     next_thing: $thing->{$each}
   print: |
        $S->{each}
        
dials:
  max_depth: 1
  form: 42

