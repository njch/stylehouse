 - K: find #c
   style: func
   D: |
      my $way = shift;
            my $point = shift;
            my @path = split /\/|\./, $point;
            my $h = $way;
            for my $p (@path) {
                $h = $h->{$p};
                unless ($h) {
                    undef $h;
                    last;
                }
