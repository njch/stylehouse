          my @nodir;
          for my $d (reverse @try) {
              if (-d $d) {
                  sayyl "$d DIR EXISTS!";
                  last;
              }
              push @nodir, $d;
          }
          say "Not existing dirs: ".wdump[@nodir];
          sayre "Weeor was $!";
          undef $!;
          say "==========";
          my $broke;
          for my $d (reverse @nodir) {
              saybl "try to make_path $d";
              mkdir $d;
              if ($!) {
                  sayre "mkdir $d: $!";
                  $broke = 1;
              }
              if (!-d $d) {
                  sayre "not exist after";
                  $broke = 1;
              }
          }
          
          sayyl "Doing again---";
           eval {
             write_file(@$arm)
           };
           if ($@) {
             say "Fucked up again: $@";
           }
          
          
          if ($broke) {
              warn "cionedddd.....\n\nWrite $file fuckup: $@";
          }
          $@ = "";
      }

      
