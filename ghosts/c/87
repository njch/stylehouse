stylepap: |
    u alljse
    y.at.jsl = 1;
    
    # push to codcoil the paper scope grab to build into the dome
    n pa =>34=><<''
        delete C.c.el;
        with (paper) {
            s = C.sc;
            var r;
            if (C.sc.pa === '3') {
                r = A.bb.pa;
                !r && a.c("no rasmeemmber "+C.t);
            }
            else {
                !s === 'ra' and a.c('nom ra: '+s);
                var src;
                s.src and src = s.src;
                if (!src) {
                    s.jfi ||= 'img';
                    var find = $(s.jfi);
                    !find.length and a.c('no find '+s.jfi);
                    src = $(find).last().attr('src');
                }
                r = new Raster(src);
            }
            s.poes and r.position = eval(s.poes);
            else
            C.sc.pa !== '3' and r.position = view.center;
            s.poxop and r.position.x += s.poxop;
            s.poyop and r.position.y += s.poyop;
            s.sca and r.scaling = s.sca;
            s.b and r.blendMode = s.b;
            s.r and r.rotation = s.r;
            if (s.opa) {
                and r.opacity = s.opa;
            }
            A.W ||= {};
            A.W.ras ||= {};
            A.W.ras[src] = r;
            
            A.W.raz ||= [];
            A.W.raz.push(r);
            console.log("Raz: are "+ A.W.raz.length);
            
            var rzl = A.W.raz.length;
            while (rzl > 9) {
                var v = A.W.raz.shift();
                rzl--;
                console.log(["rm ",v.image.src])
                v.remove();
            }
            
            A.bb.pa = r;
        }
    
Tarp: |
    u urw
    y.at.sq = 'wave';
    #y.at.dis = 0;
    #
    J.VV.N = 1;
    for (J.slow ? (3,6,5) : (1,2,3)) {
    
    J.tea ||= [shuffle grep /^[\/\-\.\w]+jpg$/i, glob 'eye/*/*'];
    
    my $pg = 'w/Tarpig';
    J.slow = readlink($pg) || 0;
    J.slow += 3;
    J.slow = 0 if J.slow >= @{J.tea};
    G&dlin,w=>J.slow,'Tarpig';
    
    #
    my $f = J.tea->[1-$_ + int J.slow];
    
    n "the J.slow J.fast"=>-pape=>{s=>'mild'=>thumb=>$f};
    
    }
