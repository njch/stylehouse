wave: | # 
    my $i = ar.i."  ";
    my $try = sub {
        my $s = shift;
        ki(7, $s);
    };
    my $M = [map{
        my $c = G&tC,$_;
        $c = G&hup,$c;
        my $z = delete c.sc.z;
        
        
        c.c = $try->(c.c);
        c.sc = $try->(c.sc);
        
        say join "\t","$i".c.t, c.y.cv, c.c, c.sc;
        $z &&
        Rw wave $N:z $i;
    }@$N];
    #my $J = G.oin;
    #n Disp=>\$M
    
    
    $N;
pi/pape: |
    C.sc.pa = 'res';
    C.sc.qz = 1;
    C.c.thumb ||= C.t;
    if (C.c.thumb) {
        m th=>-thumb=>C.c.thumb
        my $th = pop @$M;
        C.sc.src = th.sc.src;
    }
    C.sc.oc = 1;
    
    
    my ($no,$i,$se) = ($1,$2,$3)
        if `uptime` =~ /([\d\.]+), ([\d\.]+), ([\d\.]+)/;
    my $fo = $no - $i;
    shuffle 1, 2, 3 for 1..int($no);
    
    C.sc.src || sayre "no src..";
    delete C&src if C.t eq C&src;
    C.sc.poxop = -70 * (rand(2) -1.9);
    C.sc.poyop = -70 * (rand(2) -1.9);
    C.sc.sca = 3.421 * rand(3) ^ 2;
    C.sc.sca += 3;
    C.sc.opa = 0.6 * (0.5 + rand 0.9);
    C.sc.b = (shuffle 'normal', 'multiply','multiply',, 'multiply',, 'multiply',, 'multiply',, 'multiply', 'screen', 'overlay', 'soft-light', 'hard-light', 'color-dodge', 'color-burn', 'darken', 'lighten', 'difference',
    
    
    'exclusion', 'hue', 'saturation', 'luminosity', 'color', 'add', 'subtract'
    #, 'average', 'pin-light', 'negation', 'source-over', 'source-in', 'source-out', 'source-atop', 'destination-over', 'destination-in', 'destination-out', 'destination-atop', 'lighter', 'darker', 'copy', 'xor'
    )[0];
    C.sc.r = int(700 * $fo);
    
    C.sc->{$_} = I.d&ron,C.sc->{$_} for grep{C.sc->{$_}} qw'poxop poyop sca opa r';
    

    
