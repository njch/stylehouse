Dlam: |
    sayyl "Sadmi Hereop $$";
    Rw iolam;
    sayyl "Sadmi Hereop $$";
    my $cb = sub { say "Dlam"; kill 'KILL', $$ };
    Rw timer $d=2 $cb;
    u urw
    
