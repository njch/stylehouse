Syl: | # swoioshy 
    my $n = Rw Dwive + $d=10;
    my $see = 'life/testSyl.s';
    if ($n == 0) {
        `cat /dev/null > $see`;
        n followtest =>W=>'%test:1',{see=>$see}
    }
    elsif ($n < 3) {
        my $say = $n == 2 ? 'corners' : 'walls';
        `echo '$say' >> $see`;
        n followtest =>W=>'%test:'.($n+1)
    }
    elsif ($n == 3) {
        n followtest =>W=>'%test:4'
        n Film =>W=>'%test:4,noJ'
        n Follower =>W=>'%test:4,noJ'
    }
    else {
        ok(0, "Runtomuch: $n");
    }
    ok(1,"Run over: $n");
