    }
    elsif ($n == 2) {
        Rw MkSle G-Slt Detailtest;
        n Detailtest =>W=>'%test:3,West:2,noJ'
        n Followe =>W=>'%test:3,noJ',{l=>'following empty file'}
    }
    elsif ($n == 3) {
        Rw MkSle G-Slt Detailtest;
