allact: |
    u allact
    n allacgts
    n all4noJ
    n 'allareact=1'=>42,<<''
        C.sc.act = 1 if !exists C.sc.act;
        C.sc.code = 'In' if !exists C.sc.code;
allvy: |
    u allact
    n allof
    n allact
    n 'allarev=y'=>42,<<''
        return if C.sc.nov;
        C.sc.v = 'y'.C.sc.v;
        C.t .= "_".C.y.cv;
ally: |
    u allact
    n allvy
    n 'allarev=yt'=>4145,<<''
