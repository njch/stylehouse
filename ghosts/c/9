allvc: |
    u allact
    n allact
    n 'allarev=c'=>422,<<''
        return if C.sc.nov;
        C.sc.v = 'c'.C.sc.v if C.sc.v !~ /^(y|sc)/;
allof: |
    u allact
    n allacgts
    # of clues encode (when of, C should onc)
    # or how this u tater got .s'd
    # gel medium
    n 'allof'=>411,<<''
        if (C.c.of) {
            !C.t || die "ofall".ki$C;
            A.J.A.vse.of = C.c.of;
            return T.not = 1;
        }
        C.sc.code ||= do {
            my $cv = C.y.cv || 1;
            $cv =~ s/^0\.//;
            my $of = A.J.A.vse.of;
            $of ||= do {
                warn "Using Ct code=C.t $cv   for ".ki $C;
                C.t
            };
            "$of $cv"
        };
    
allants: |
    u allact
    n allof
    n allact
    # general things, D.t mangles out
    allants 415: %code:In
        C.t = A.J.name . 0 . A.J.A.vse.ants++ . 0 . C.t;
    
allt: |
    u allact
    n allants
    # D neck sc.t expr, usu match D.t for C.t etc. loaf out, ref up
    allt 414: %code:In
        C.sc.t ||= 't';
        C.sc.Ct ||= C.t if C.sc.t eq 't';
    
allvse: |
    u allact
    n allvc
    n allof
    # nov not env
    n 'allvse'=>419,<<'','%code:In'
        if (C.sc.nv) {
            C.sc.nov = 1;
            C.sc.v = C.sc.nv;
        }
        C.sc.v ||= C.t unless exists C.sc.v;
        (my $cv = C.y.cv) =~ s/^0\.//;
        $cv = $1 if !$cv && C.sc.code =~ /^\w+ (\d+)$/;
        $cv||die"no0 C.y.cv  ".ki 2, $C;
        C.t .= "_$cv";
