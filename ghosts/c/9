
allcode: |
    u allact
    n all4noJ
    n 'allarecode=1'=>42,<<''
        C.sc.code = 1 if !exists C.sc.code;
all4noJ: |
    u allact
    n 'skip4around4forJ'=>4,<<''
        return unless C.c.J || C.c.W || C.c.pi;
        T.Z->{0.44} = 1;
        T.not = 1;
    
allpack: |
    u allact
    n 'allarepack=t'=>41,<<''
        C.sc.code = 'Pack' if !exists C.sc.code;
        C.sc.pack ||= C.t;
allacgt: |
    u allact
    n allcode
