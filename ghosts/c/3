csdig: |
    
     for my $n (@$ln) {
         my $s = $n->[2]; # t/y/(s)
         $s = ref $s eq 'HASH' ? "s.r s.id"
            : ref $s eq 'ARRAY' ? die 'arline'
            : $s;
         $n->[2] = $s;
     }
     my $d = ejson $ln;
     #saygr " Digwa: ".slim 280, $d if ar.V;
     $d = dig $d;

