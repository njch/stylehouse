        # stackable A with stitchable others interleaved in time
        # 6 FLIES
        o.M = [grep{defined}map{
            my $d;
            d.t = _.t;
            d.y = {%{_.y}};
            d.c = {%{_.c}};
            delete d.y.cv;
            d.sc = {%{_.sc||{}}};
