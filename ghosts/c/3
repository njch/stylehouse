lum: |
    u allvy
    
    n lum_thy =>''=>{s=><<''},{code=>'Mo 233',v=>'lumfa',rg=>'bal'}
        # barel! vylum
        # could put these mys on y.u.thing and loop Nautically in 32...
        my $roll = 1;
        my $lum = 3;
        my $i = 0;
        for (@$bal) {
            _.y.thy+=$lum;
            $i++;
            if ($i > $roll) {
                $lum -= $lum * 0.2 if $lum > 0.1;
            }
        }
    
    n spc_lum =>''=>{s=><<''},{code=>'Mo 37',v=>'lum',rg=>'spc'}
        # feel effects of... f eel
        my $upt = 0;
        $spc = $upt if $spc < $upt;
        # t-rewirey formulae:
        my $fs = ($spc / 0.02) * 4;
        C.sc.fs = $fs if $fs < 9;
        C.y.spc = $spc;

