Yeosleep: | # notes in V.os of C, sees if it wants to wake up...
    u allvse
    n '','','%of:Sev'
    
    n Y =>23=><<'',{rg=>'J,V'}
        sayyl "Was in! ".ki $C;
        C.y.k = C.y.cv."\t".C.t;
        push @{V.os}, C.y.k unless C.c.root;
    
    n Y =>24=><<'',{rg=>'J,V'}
        return if C.c.root;
        # if change is, or just diff in perceiving it
        my $toen = 1;
        .
        if (my $di = Y.lo->{C.y.k}) {
            my $not;
            if (keys %$di == 1 && di.diff) {
                my $CC = V.tv->{C.y.k};
                if (!$CC) {
                    sayre "never heard of (V_tv) diff J.name : C.y.k ";
                    return 1;
                }
                for my $tk (keys %{di.diff}) {
                    if ($tk eq 'geo') {
                        $not++ if 0 && !I.d&heq,C.sc.geo,CC.sc.geo;
                        if ($not && 0) {
                            sayyl "Geo was $not a difference in the thing";
                            sayyl sjson(C.sc.geo);
                            saybl sjson(CC.sc.geo);
                        }
                    }
                    elsif ($tk eq '') {
                        $not++ unless ref C.c.s && C.sc.dige && C.sc.dige eq CC.sc.dige;
                    }
                    else {
                        saybl "J.name C.y.k has diffre: $tk";
                        $not++;
                    }
                }
            }
            else {
                $not = 1;
            }
            $toen = 0 if !$not;
        }
        .
        if (!$toen) {
            if (my $JJ = C.sc.J) {
                my $ta = V.ta->{C.y.k};
                if (my $nV = JJ.El->{V.name}) {
                    my $iv = nV.rv;
                    my $oi = ta.V->{V.name};
                    if ($oi ne iv.i) {
                        $toen = 1;
                        #sayyl "J.name  $k   about JJ.name: $toen \n"
                        #.($toen&&" ^ iv.i\n | $oi");
                    }
                }
                else {
                   sayre "no El V.name on JJ.name";
                }
            }
        }
        .
        return T.not = 1 if !$toen;
        
