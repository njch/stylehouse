Sev: |
    my ($A,$I) = G.h&Sev;
    my $i;
    while (@{A.N}) {
        $i++ > 1000 && die "Huge A.am";
        my ($A,$C,$T) = I.d&Act,$A;
                T.not&&next;
        A.t&6;
                T.not&&next;
        I.d&flywheels;
                T.not&&next;
        A.t&78;
                T.not&&next;
    }
    continue { I.d&z; }
    A.t&8;
    I.d&recycle;
