        `mkdir -p $dir` if !-d $dir;
        my $code = join "\n", @{t.l};
        my $f = "$dir/$fi\.pm";
        write_file($f, $code);
        push @inc, t.pack;
    }
    n othing=>''=><<'','%code'
        say "Not nothing here";
    
    
