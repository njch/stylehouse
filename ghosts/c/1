SNtl: | # spiral Nat toolz
    I.n = sub { # A qcui
        my ($A,$C,$G,$T,$AA,$CC,$GG,$TT) = @_;
        I.nF = {A=>$AA,C=>$CC,G=>$GG,T=>$TT};
    };
    I.d = sub { # be, sub
        my $s = shift;
        my $P = I.nF ||= {};
        my $y = $I->{$s} || die "no way named '$s'";
        $y->(P.A,P.C,P.G,P.T,@_);
    };
    I.An = sub { # A within faller # wants to I 2, throw subs on A directly...
        my ($A,$C,$G,$T) = @_;
        A.note = {}; # pinging csc
        A.c = sub { die "An ein!" };
        A.t = sub { I.d&t,@_ };
        $A
    };
    I.t = sub {
        my ($A,$C,$G,$T,@M) = @_;
        my ($cv,$K) = reverse @M;
        @M>2 && die "_ _";
        $K ||= A.am||die"noam A_t";
        
        my $i = Rw collaspII $A;
        
        my $c = 0+("0.".$cv);
        sayyl "Sesnbitall A.J.name A.t->I.t";
        die "Calling Essentall A.t->I.t for $c" if $c > 0.1;
        die "nowlow $c" if $c < 0.1;
        say "A.J.name  ex $i up to  $c";
        Rw SNat $A $C $G $T $I $i $K $cv;
        A.cv = $c;
    };
