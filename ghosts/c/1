        my $was = A.cv;
        $dont || I.d&ex,$i,$K,$cv,$av;
        A.cv = $ncv if defined $ncv && A.cv == $was;
        $re;
    
    n ex =>''=>{s=><<''},{acgt=>'i,K,cv,av',nois=>'#c'}
