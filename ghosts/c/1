            # want to sus $@ and hang vectors of what, should be autoish
            my $m = "J: I.nF.A.talk        A.cv";
            $m .= "\nC: ".k2 I.nF.C;
            $m .= "\ns: ".wdump 3, I.nF.A.s unless length $m > 20;
            $@ = "$m\n$@";
            # A.t&9,$@; # skippy
        }
    
