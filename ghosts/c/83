PSF: |
    my $j;
    for (`ps faux | grep perl`) {
        next if /grep perl/;
        my ($user,$pid,$cpu,$mem) = split /\s+/;
        my $p = $1 if /\|  (.+)$/;
        $j .= "$cpu  $mem \t !  $p\n";
    }
    
    n ps=>-pre=>$j
