     -
      - is # colour from value spinner
      - 3.2
      - |
        my ($t,$z) = split ':', $s;
        $z ||= C.c.s;
        my $i = sum map { ord $_ } split '', $z;
        $i -= 9 until $i < 10;
        C.sc.hs = "${i}75";
     -
      - mu # timers
      - 3.2
      - |
        if ($s eq 'g') {
            C.c.s || die "nos : ".wdump 2, $C;
            C.c.s = Rw mugwu $d:C.c.s;
            C.sc.hs ||= "576";
        }
        else {
            die "how mu $s? C.s";
        }
