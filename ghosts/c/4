TheAttractor: |
    u allsvse
        
    m @$_
    for @{Load(<<'')}
     -
      - sz
      - 3.1
      - |
        my $si = length(C.c.s);
        ($s, my $lim) = split '/',$s;
        my $di = $s/$si;
        $di = $lim if $lim && $di > $lim;
        I.d&meat,fsi=>$di;
        I.d&meat,fsi=>1 if $di > 2;
        return;
        #        $fs = "font-size:$fs%;";
        #        if ($si > $tev) {
        #            $text = ($text=~/^(.{$tev})/s)[0];
        #        }
