trowil: |
    my @li = glob "$s/*/*";
    my $tw = {};
    for my $f (@li) {
        my ($gate,$up,$way) = $f =~ /^(.+)\/J\/(.+?)\/(.+?)$/;
        $up && $way || die "nope $f";
        $tw->{$up}->{n}->{$way} = 1;
        $tw->{$way}->{u}->{$up} = 1;
    }
    my $wt = {};
    $wt->{$_} = {} for grep{!$tw->{$_}->{u}} keys %$tw;
    my @ne = $wt;
    while (@ne) {
        my @m = @ne;
        @ne = ();
        for $wt (@m) {
            for my $k (keys %$wt) {
                my $t = $tw->{$k};
                if (t.n) {
                    my $n = $wt->{$k};
                    $n->{$_} = {} for keys %{t.n};
                    push @ne, $n;
                }
                else {
                    #
                }
            }
        }
    }
    J.mo.jes.tower = $tw;
    J.mo.jes.trowl = $wt;
