    n hsle =>''=><<''
        my @m;
        for ($s) {
            @m = split ' ', $_;
            last if @m == 3 || @m == 4;
            @m = split /\./, $_;
            last if @m == 3 || @m == 4;
            @m = split '', $_;
            last if @m == 3 || @m == 4;
            @m = ();
        }
        @m || die "not hsle: $s";
        my $h;
        (h.h, h.s, h.l, h.a) = map{0+("0.".$_)}@m;
        .
        h.h = h.h * 360 if h.h < 1;
        .
        $h->{$_} = ($h->{$_}*100).'%' for 's','l';
        .
        my $al = ", h.a" if defined h.a;
        "hsl".($al?'a':'')."(h.h, h.s, h.l$al)";
    
