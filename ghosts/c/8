        Rw wayt $J $C $d=0.01 $p=jive;
    
    my $ooJ = J.oJ->{oJ.name} ||= {};
    # is {M:name,W:id} coupling, so pi is {file:g/c/1} etc
    my $Y = oJ.El.Duv;
    my $rv = Y.rv;
    if (@{rv.sc}) {
        sayre "J.name SLIPPING SC" for 1..5;
        sleep 4;
    }
    if (rv.i eq ooJ.rv.i) {
        say "J.name receiving same oJ.name";
        return;
