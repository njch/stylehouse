Rums: | # as an In osc, string ring ring
    # attach _.yy.NOISE.ytalk for tiny ysland mentalities
    @$M = map {
        my $ls = int($spc * 90);
        my $lino = J.mo.yam.sc.vert || scalar(split"\n",_.c.s);
        saybl "Have $spc * 90 = $ls      ----- $lino";
        if ($lino && $lino > $ls) {
            _.c.s =~ s/^(([^\n]*\n){$ls}).*$/$1 .../s;
            saybl "Made snip";
        }
        if ($spc < 0.2) {
            $_
        }
        elsif (_.c.s =~ /^(.+?): (.+)$/) {
            $_
        }
        elsif (_.c.s =~ /^(.+)( sc=\{ )(.+)$/s) {
            I.d&xoy,{c=>{s=>$1}},$_ ,
            [and=>''=>"AND: $2"=>'%hs:483'], 
            I.d&xoy,{c=>{s=>$3},sc=>{hs=>'287'}},$_
        }
        else {
            $_
        }
    } @$M;
