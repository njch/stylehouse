    dlin: %acgt:dir to it
        my $l = "$dir/$it";
        my $is = -l $l;
        return if $is && readlink($l) eq $to;
        `unlink $l` if $is;
        `cd $dir; ln -s $to $it`;
    
    wraf: %acgt:f s noappend
        $s =~ s/\n?$/\n/s;
        G&writef,$f,$s,!$noappend
    
    writef: %acgt:f s append
        (my $d = $f) =~ s/\/[^\/]+?$//;
        unless (-d $d) {
            `mkdir -p $d`;
        }
        .
        $s = [$s] if !ref $s;
        my $li = ref $s eq 'ARRAY' ? join("",map{/\n$/?$_:"$_\n"}@$s)
            : die "Wsome other $f type: $s";
        .
        my $wa = {};
        wa.append = 1 if $append;
        write_file($f,$wa,$li);
        return length $li;
    
