    #  - way $k digs into w/digway/$kish
    #    - new file written & diffed and move in/over w/way/$kish
    
    my $glomet = sub {
        my $f = shift;
        my $mt = (stat $f)[9];
        my $fi = $f;
        $fi =~ s/\//-/g;
        my $wf = "w/wf/$fi";
        my $mft = (stat $wf)[9] if -f $wf;
        $mt > $mft
    };
    
    my @li = glob $p;
    for my $f (@li) {
        $glomet->($f) || next;
