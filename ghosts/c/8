    # attach _.yy.NOISE.ytalk for tiny ysland mentalities
    @$M = map {
        my $ls = int($spc * 90);
        my $lino = J.mo.yam.sc.vert || scalar(split"\n",_.c.s);
        saybl "Have $spc * 90 = $ls      ----- $lino";
