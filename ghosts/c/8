        }
        if ($spc < 0.2) {
            $_
        }
        elsif (_.c.s =~ /^(.+?): (.+)$/) {
            $_
        }
        elsif (_.c.s =~ /^(.+)( sc=\{ )(.+)$/s) {
            I.d&xoy,{c=>{s=>$1}},$_ ,
            [and=>''=>"AND: $2"=>'%hs:483'], 
            I.d&xoy,{c=>{s=>$3},sc=>{hs=>'287'}},$_
        }
        else {
            $_
        }
    } @$M;
    saybl "Took ".F_delta()." to spread J.name 's ".@$M;
    
