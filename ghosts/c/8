ll: |
    my $se = `ls -l --full-time $s`;
    $se =~ s/-rw-r\S-r-- 1 s s //sgm;
    my $l = [map {
        if (/^(\s*)(\d+) ([\d-]{10}) ((?:\d+:?)+)\.\d+ \+\d{4} (.+)$/) {
            "$1$2 -- $4 -- $5";
        }
        else {
            $_
        }
    } split "\n", $se];
    $l
