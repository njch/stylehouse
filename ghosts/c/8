pi/websock: |
    my $tx = $s->tx;
    Mojo::IOLoop->stream($tx->connection)->timeout(300000);
      $s->on(message => sub {
          my ($M, $m) = @_;
          eval { Sw speaks[$m]; };
          sayre "Elvis-induced error ".$S->pi."  $@" if $@;
          $@ = "";
      });
      $s->on(finish => sub {
          my ($M, $code, $reason) = @_;
          Sw leaves[$code, $reason];
      });
      B.address = $tx->remote_address;
pi/tfile: |
    n 1,1,Tailing =>'%hs:4856'
