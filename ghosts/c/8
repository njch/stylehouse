SpaceElvis: |
    my $M = J.oy.o;
    Mn($_) for @{J.eM||[]};
    J.eM = $M;
    for (A.e&C) {
        # y.unit 
        my $id = _.sc.id;
        if (grep{_.sc.id eq $id}@$M) {
            saygr "Already spaced: $id";
            next;
        }
        sayyl "GOT SPACE ELVIS: C.t: _.t   _.y.cv";
        Mn(_.t=>''=>"Thi _.t",{id=>$id});
        # & id ^
    }
