waitme: | # 
    use POSIX ':sys_wait_h';
    if (G.forked) {
        sayre "waitme already forked" for 1..3;
        return;
        exit;
    }
    
    my $parent = $$;
    if (my $pid = fork()) {
        sayyl " ,,,,";
        sayyl "FOrked!";
        sayyl " ,,,,";
        my $and;
        my $kill = $SIG{INT} = sub {
            sayre "Killing $pid...";
            kill 'KILL', $pid;
            delete $SIG{INT}
        };
        $SIG{HUP} = sub {
            $and = 1;
        };
        
        waitpid($pid,0);
        
        if ($and) {
            saybl "child wants restart!";
            return
            Rw waitme
        }
        saybl "Restart? <";
        my $IN = \*STDIN;
        while (my $k = <$IN>) {
            chomp $k;
            J.me = $k;
            say "__ $k";
            Rw goche $n=$1
            while $k =~ s/(\d+)//g;
            Rw goche $n:_
                for qw'81 64 8 84 1';
            Rw waitme;
            return;
        }
    }
    else {
        G.forked = $parent;
    }
exit: |
    for (@{ar.m||[]}) {
        if ($_ eq 'refork') {
            G.forked eq $$ && die "You are forked: $$";
            kill HUP=>G.forked;
        }
    }
    exit;
