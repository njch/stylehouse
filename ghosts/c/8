    Mn(Joint=>-ll=>{ll=>'join',s=>"$s/J/*"},'%hs:399');
    
pi/join: |
    $s =~ /(\S+J\/(\w+)\/(\w+))$/;
    my $jf = $1;
    my $fj = "(!-f)" if !-f $jf;
    my $jfm = (stat $jf)[9];
    my $wf = "w/way/$3";
    my $wfm = (stat $wf)[9];
    !-f $wf &&
        n no=>''=>" NO $wf",'%hs:846'
    my $del = $wfm - $jfm;
    $del < 0 && return
        n del=>''=>" OK: $del",'%hs:288'
    
    n sout => 3 =>"\n - Sou $wf ($wfm - $jfm)  $jf",'%hs:394,fs:5'
    n means => 4 => $del,'%hs:445'
