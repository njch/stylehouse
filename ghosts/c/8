ghosch: |
    return sayre "ghosch iggy $m" if $m !~ /^g(hosts?)?\/c\//;
    my $u = LoadFile $m;
    map {
        (my $k = $_) =~ s/\W/-/g;
        G.way->{$k} = $u->{$_};
    } keys %$u;
    delete G.drop;

