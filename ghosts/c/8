    sayyl "Reading for $s" for 1..4;
    push @l, {};
    my ($t,$s,$e,$i);
    for my $l (@l) {
        if (ref $l || $l =~ /^(\S.*): \|(.*)\n/) {
            my @new = ($1,'',$2);
            if (defined $t) {
                my $ind = $1 if $s =~ /^(\s+)\S/ || die "Noin stya: $l";
                $s =~ s/^$ind//sgm;
                my $sc;
                sc.vert = $i;
                sc.ontop = $e if length $e;
                sc.dig = slm(6, dig($s));
                Mn($t=>'pi:yam'=>{s=>$s},$sc);
            }
            ($t,$s,$e) = @new;
            $i = 0;
        }
        else {
            $l !~ /\S/ && next || die "no t aft $l" if !$t;
            $s .= $l;
            $i++;
        }
    }
