    -f $s || die "NOpe -f $s";
    my $al = J.al ||= {};
    
    ref C.c.cb eq 'CODE' || die "No callback to J.name";
    
    al.s ||= do {
        open my $ha, '-|','tail', '-q',
            '-s','0.1','-F','-n0',
            $s
            or die $!;
        #al.s && al.s->close;
        my $S = al.s = Mojo::IOLoop::Stream->new($ha);
        $S->on(read => sub { C.c.cb->($_[1]); });
        $S->on(close => sub { die "$s closed!?"; });
        $S->on(error => sub {
            my ($s, $err) = @_;
            die "$s err: $err";
        });
        $S->timeout(0);
        $S->start;
        #$s->reactor->start unless $s->reactor->is_running;
        $S
    };
