pi/Join: |
    C.sc.dos = 'pileJup';
    my $dir = $s;
    -d $dir || die "No $dir";
    my $p = "$dir/*";
    n Join=>1=>$s,'%decor,hs:388'
    J.mo.jes = $J;
    
    my $geese = sub {
        my $f = shift;
        my $way = $1 if $f =~ /.+\/(.+?)$/;
        $way || die "noqway $f";
        my $mt = (stat $f)[9];
        my $fi = $f;
        $fi =~ s/\//-/g;
        my $wf = "w/way/$way";
        my $wt = (stat $wf)[9] if -f $wf;
        $wt > $mt
    };
    
    my $li = {map{$_=>1}glob($p)};
    my @n;
    my $jf = "$dir/J";
    if (delete $li->{$jf}) {
        Rw trowil $J $s:jf;
        Mn($jf=>-gejo=>{}=>'%dis');
        
    }
    else {
        sayre "No $jf in ".wdump 2, $li;
    }
    my $wt = J.mo.jes.trowl || die "no trowl";
    
    my $tal;$tal = sub {
        my ($d,$wt) = @_;
        map {
            my $v = $wt->{$_};
            my @M;
            push @M, [$_=>''=>{s=>$_,ind=>$d}];
            push @M, $tal->($d+1, $v) if keys %$v;
            @M
        } sort keys %$wt;
    };
    my $N = [$tal->(0,$wt)];
    my $eM = [];
    Rw In $J $N $M:eM;
    
    for (@$eM) {
        my $f = "$dir/_.t";
        if (delete $li->{$f}) {
            _.sc.ml = _.c.ind * 3;
            if ($geese->($f)) {
                _.c.s = $f;
                _.c.pi = 'gee';
            }
            else {
                _.c.pi = 'bl';
            }
            _.sc.id = $f;
            _.sc.hs = '255';
        }
        else {
            _.sc.hs = '339';
        }
