jogthe: |
    Rw jeocling;
    y.at.jsl = 1; # send js
    n Swomp
    n Shorse
    n Sipes
    
jeocling: |
    J.pix = [{any=>1,Rw=>'jeocling'}];
    u jseo
    u trWs
    y.at.isle = 'jes';
trWs: |
    u allvse
    n '','','%of:Mo'
    n W =>3211,<<''
        unless (A.J.C.sc.pi eq 'trW') {
            C.c.s = delete C.c.W;
            C.c.pi = 'trW';
        }
    
pi/trW: | # uses localeo
    my $isle = J.mo.J.oy.at.isle;
    $isle || die "No isledea !? J.name of J.mo.J.name";
    my $f = "w/$isle/$s";
    #`rm $f`;
    if (-f $f) {
        say "Found $f !";
        n $f =>3,$s,{X=>LoadFile($f)}
    }
    else {
        say "Making  $s !";
        n $s=>W
    }
    my $Jd = "w/$isle/J/J.mo.J.name";
    `mkdir -p $Jd` if !-d $Jd;
    `touch $Jd/$s`;
    saybl "Donel  $Jd  $s";
    
Sweep: |
    n stylesweep
stylesweep: |
    u alljse
    n hs =>44,<<''
        $.each(s.thy,function(k,v){
            C.sc[k] and C.sc.css[v] = C.sc[k];
        });
        $.each(s.roi,function(k,v){
            var o = C.sc.css[k];
            !o and return;
            var ve = v[0];
            var ss = v[1];
            C.sc.css[ve] ||= {};
            C.sc.css[ve][ss] = o;
            delete C.sc.css[k];
        });

