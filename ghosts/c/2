    #for [@{Y.rv.c},@{Y.rv.sc}];
    
    say F_delta();
    # wants to flatten to clifes, keeping pathT/t/t/t , watching pha cones
    #$J = $oo;
    # curve falls # just throw J.Idex on
    
    
    my $N = [
        @{Y.rv.c},
        @{Y.rv.sc},
    ];
    $N = [{MZ=>'wongui'}, @$N];
    my $M = [];
    Rw In $J $N $M;
    $N = $M;
    
    my $want = [[sc=>'code']];
