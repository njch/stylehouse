    # u/n/m get here, we know ar.
    # here we step further into the light grabbing
    # parts spine up from In's A the In, using qqJ.A
    # but the broken tiles of use cassettes- culture eddies, vines of intuit
    I.An = sub {
        my ($A,$C,$G,$T) = @_;
        A.note = {}; # pinging csc
        A.interpD = sub { # make csc grab exprD
            my ($nk,$gk) = @_;
            my $D = {};
            if (ref $nk) {
                $D = $nk
            }
            else {
                while ($gk =~ s/^%(.+?):(.+?)(?=%|$)//g) {
                    D.c->{$1} = $2;
                    pop @_ if @_ == 2;
                }
                if (@_ == 1) { # or from t,,,
                    D.c.nk ||= 'c';
                    D.c.gk ||= $nk;
                }
                elsif (@_ == 2) {
                    D.c.nk = $nk;
                    D.c.gk = $gk;
                }
            }
            $D
        };
        A.c = sub { # 
            my ($D) = @_;
            $D = A.interpD->(@_) if !ref $D;
            
            exists C.c->{$_} && die "COT INTERESTING ".ki $C for qw'code if of ref';
            
            my $v = I.d&Cngk,$D;
            return () unless $v;
            
            A.note->{D.c.nk}->{D.c.gk} = $v if A.note;
            
            die 'mulitAn' if @$v > 1;
            if (D.c.as eq 'list') {
                return ref $v eq 'ARRAY' ? @$v : die'Cngk not array';
            }
            return 1;
        };
        A.t = sub {
            @_>2 && die "_ _";
            my ($cv,$K) = reverse @_;
            $K ||= A.am||die"noam A_t";
            if (my $i = J.A.I) {
                I.d&ex,$i,$K=>$cv
            }
            # man 
        };
        $A
    };
    I.ex = sub {
        my ($A,$C,$G,$T,$i,$K,$cv) = @_;
        # A wind down to K cv
        saybl "J.name C: ".ki $C;
        for my $k (sort keys %$i) {
            my $ii = $i->{$k};
            die "$K $cv - $k" if $K ne $k;
            for my $vc (sort keys %$ii) {
                my $iii = $ii->{$vc};
                die "$K $cv - $k $vc" if $cv ne $vc;
                for my $ik (sort keys %$iii) {
                    my $iv = $iii->{$ik};
                    say "$K $cv - ".ki $iv;
                }
            }
        }
    };
