


scgkindex: | # make scindex
    D.sc.scindex = {map{_.sc.gk=>1}grep{_.sc.nk eq 'sc' && _.sc.gk}@$N};
jshang: |
    y.at.isle = 'jes';
    y.at.jsl = 1; # send js
    Rw ocling;
    
    n Shorse
    n Sipes
    n Swomp
    
Shorse: |
    n stylehut
    n styleven
    n styleomi
Sipes: |
