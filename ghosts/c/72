Viu: |
    my $eu = sub { J.uS = 0 if J.uS };
    my $unu = sub {
        return if J.uS;
        Rw uCon;
        J.uS = 1;
    };
    my $dJ = sub {
        my @m = @_;
        if (@m < 4) {
            $m[2] ||= {J=>$m[0]};
            @m == 3 || die "!!!\n".wdump[@m];
        }
        while (@m > 4) {
            my $wat = pop @m;
            $wat eq '' || die "got some $wat  f n".wdump 2, \@m;
        }
        [@m]
    };
    J.u = sub {
       $eu->();
       my $ar;
       ar.N = [$dJ->(@_)];
       ar.J = Rw j $J qq;
       ar.J.V = 1;
       sayyl "J.name U! @_";
       Rw MyN;
    };
    J.n = sub {
       $unu->();
       my $ar;
       ar.J = $J;
       ar.N = [$dJ->(@_)];
       Rw MyN;
    };
    J.m = sub {
       $unu->();
       my $ar;
       ar.M = shift;
       ar.J = $J;
       ar.N = [$dJ->(@_)];
       Rw MyN;
    };
