Jto: |
    my $J;
    J.id = mkuid;
    J.name = ar.name || (@{delete ar.m})[0];
    J.bb = {};

    J.most.J = $J;
    J.most.mody = 'mani';
    J.most.eo = 'Yesui';
    J.tt = 1;

    J.u = sub {
       my $ar;
       ar.J = Rw j $J qq;
       ar.N = [[@_]];
       sayyl "J.name U! @_";
       Rw MyN;
       # & concert overworld
    };
    J.n = sub {
       my $ar;
       ar.J = $J;
       ar.N = [[@_]];
       Rw MyN;
    };
    J.m = sub {
       my $ar;
       ar.M = shift;
       ar.J = $J;
       ar.N = [[@_]];
       Rw MyN;
    };
    J.bz = sub { #
       my $m = [@_];
       die "not 2 @$m" if @$m != 2;
       my $j={};
       while (@$m) {
           my ($r, $v) = (shift @$m, shift @$m);
           if ($r =~ /^%(\S+)/) {
               j.style = $1;
           }
           else {
               $j = Rw r2j $r $J;
           }
           defined j.cv || die "NOSEYV: j.cv".wdump[$j,[@_],[$r, $v]];
           
           j.r = join "\t", j.cv, j.t, j.style;
           j.s = $v;
           my @Js = $J;
           push @Js, values %{J.phy} if J.phy;
           for my $jj (@Js) {
               jj.bb->{j.r} = j.s;
               push @{jj.ord}, {%$j} if jj.ord;
               jj.tim->{j.r} = hitime() if jj.tim;
               jj.wtf->{j.r} = $F[0] if jj.wtf;
               jj.ap->($j) if jj.ap; # <<<
           }
      }
    };
    $J
