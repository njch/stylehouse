             Jn('ends',0.2,'@','{hs:6984}')
             if J.d.y.spc < 0.07;
            
            return if $cv < 0.2;
            
            my $i = 0;
            while (exists $u->[$i]) {
                my $uu = $u->[$i];
                
                
