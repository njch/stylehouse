    return saygr "No\ndown\nsave: $t" unless -f $f;
    my $datya = LoadFile($f);
    die "Eval $f: $@" if $@;
    die "Not dataray $f $datya" if ref $datya ne 'ARRAY';
    push @{j.UG}, map {saybl "Resuyming $t: ".ki $_;$_} @$datya;
