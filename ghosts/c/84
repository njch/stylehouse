pi/Join: |
    C.sc.dos = 'pileJup';
    my $dir = $s;
    -d $dir || die "No $dir";
    n Join=>0.0001=>$dir,'%decor,hs:388'
    my $jes = J.mo.jes = $J;
    C.c.isley = $1 if $dir =~ /.+\/(.+?)$/
        || die "DirJ not k ended: $dir";
    
    my $geese = sub {
        my $f = shift;
        my $way = $1 if $f =~ /.+\/(.+?)$/;
        $way || die "noqway $f";
        my $mt = (stat $f)[9];
        my $fi = $f;
        $fi =~ s/\//-/g;
        my $wf = "w/way/$way";
        my $wt = (stat $wf)[9] if -f $wf;
        $wt > $mt
    };
    
    my $li = {map{$_=>1}glob("$dir/*")};
    my @n;
    my $jf = "$dir/J";
    if (delete $li->{$jf}) {
        Rw trowil $J $s:jf;
        Mn($jf=>-gejo=>{}=>'%dis');
        
    }
    else {
        sayre "No $jf in ".wdump 2, $li;
    }
    my $wt = jes.trowl || die "no trowl";
    my $tal;$tal = sub {
        my ($d,$wt) = @_;
        map {
            my $v = $wt->{$_};
            my @M;
            push @M, [$_=>''=>{s=>$_,ind=>$d}];
            push @M, $tal->($d+1, $v) if keys %$v;
            @M
        } sort keys %$wt;
    };
    my $N = [$tal->(0,$wt)];
    my $eM = [];
    Rw In $J $N $M:eM;
    
    my $was = {};
    for (@$eM) {
        my $f = "$dir/_.t";
        if (delete($li->{$f}) || $was->{$f}) {
            _.c.s = $f;
            _.sc.ml = _.c.ind * 3;
            if ($geese->($f)) {
                _.c.pi = 'gee';
            }
            else {
                _.c.pi = 'bl';
            }
            if (my $aw = $was->{$f}) {
                if (aw.c.ind < _.c.ind) {
                    _.c.pi = 'gefore';
                }
                else {
                    aw.c.pi = 'gefore';
                    $was->{$f} = $_;
                }
            }
            else {
                $was->{$f} = $_;
            }
            _.sc.id = $f;
            _.sc.hs = '255';
        }
        else {
            _.sc.hs = '339';
        }
        _.sc.dis = 1;
    }
    
    my $la;
    for (1..3) {
    for (@$eM) {
        if (_.c.pi eq 'gefore') {
            if ($la && la.c.pi eq 'gefore') {
                if (la.c.ind + 1 == _.c.ind) {
                    _.c.dropi++;
                }
            }
        }
        $la = $_;
    }
    @$eM = grep{ keys %$_ } grep { !_.c.dropi } @$eM;
    }
    my $ee = jes.teeps = {};
    my $un = {};
    my @ins = grep {_.c.pi ne 'gefore'} @$eM;
    $ee->{_.t} = $_ for @ins;
    $un->{_.t}++ for @ins;
    my @to = grep { $un->{$_} > 1 } keys %$un;
    warn "things showed up too many; @to" if @to;
    
    Mn($eM);
    
    keys %$li &&
        Mn(unc=>''=>"Uncounted: $_\n",'%ws,dis')
            for sort keys %$li;
    
    saybl _.c.s for @$M;
    
    n o=>62=>"@",'%fs:40,blr:4';
    # and so
