Gapitan: |
    J.A.IIII = ['G/T/Remy'];
    u allgap
    n Goo =>W
    
allgap: |
    # make O throw a flywheeled-to-t1ex-G perl file down
    n 'allgap'=>2,{s=><<''},{code=>'S 42'}
        C.sc.acgt = '1' if !exists C.sc.acgt;
Perl: |
    n PerlHead =>W
    n IStuff =>W
    n KnowTime =>W
    J.V = 1;
    
