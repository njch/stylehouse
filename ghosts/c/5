Oth: |
    # rising 
    y.ope = 1;
    J.A.tid++;
    n All
    
    'Oth'ne$s&&
    n $s
    # randonly pour time ^
    T.nose = 1;
    
All: |
    J.mo.s.al = $J;
    n Byst
    n Check
    n @$_ for map{[$_=>W=>{}]} map { sayyl "All uip _.name"; _.name }
        map { J.mo.s.bb->{"$_\tJ"} } 
        grep {$_} map { /^(.+)\tall$/ && $1 } keys %{J.mo.s.bb};
        
    
    
Byst: | # the random for news interro
    G.Byst = $J;
    n s=>''=>t;
    
Check: |
    Rw Loading;
    n wa =>''=>{pi=>'waps'}
    
    
Rig: | # Wild
