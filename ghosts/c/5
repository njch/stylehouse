dumpGoinbb: |
    my $bb = G.oin.bb;
    my $gg = G.oin.ddlwpp ||= {};
    saygr wdump 1, [ map{ 
        my $o = $gg->{$_};
        my $v = $bb->{$_};
        $gg->{$_} = $v;
        "$_:    $bb->{$_} ".($o ? $o ne $v ? "(o: $o)" : "(o)" : "");
    }grep{/J$/}sort keys %$bb];
