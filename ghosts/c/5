TestTool: |
    u allacgts
    
    # pushed to
    n taccept =>'',<<''
        my $f = C.c.tfile;
        (my $d = $f) =~ s/\/[^\/]+$//;
        `mkdir -p $d` unless -d $d;
        my $got = C.c.st.got;
        say "Write $f the ".ki $got;
        DumpFile($f, $got);

    n eeply =>'',<<'',{acgt=>'s,K'}
        C.c.st.got->{$K} = $s;
        my $e = C.c.st.exp->{$K};
        is_deeply($s, $e, $K);
        
    n testNews =>'',<<''
        # provide $vl for follower
        my $rv = $s;
        my $rl = {}; # pass, lives with $ids
        my $r = Rw scd $rl $rv;
        my $o = Rw ZNews sold $sc:r.oldz if r.oldz;
        my $n = Rw ZNews news $sc:r.z;
        my $stuff = $n;
        my $ids = {};
        my $here = 3;
        my $idi = 1;
        my $idstu = sub {
            my $id = shift;
            my $il = $ids->{$id} ||= $idi++;
            "il$here$il";
        };
        $stuff =~ s/(id=)(\S+)/$1.$idstu->($2)/eg;
        $stuff =~ s/(mtime=)(\d{5})(\d+)/$1.$2.join'', (('?') x length $3)/eg;
        [split "\n", encode_utf8 $stuff];
    
