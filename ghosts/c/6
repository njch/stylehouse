Foldout: |
        my @m = ref $x eq 'ARRAY' ? @$x : $x;
        for my $x (@m) {
            if (x.pi ? C.c.pi eq x.pi : 
                x.any ? 1 :
                0) {
                saygr "matched: J.name ::: ".ki $x;
                
                x.u &&
                u x.u
                
                C.c.pi = C.sc.pi = x.pito if x.pito;
                
                x.Rw &&
                    Rw x.Rw;
            }
            # up!
        }
