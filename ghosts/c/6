                A.ico.qui = 1;
                I.d&as,A.ico,A.coi;
                my $wtf = I.d&exactl,A.ico;
                .
                if (wtf.last && !wtf.same) {
                    my $uf = join "\n", map{"$_: ".wdump(1,$wtf->{$_})}
                        grep {keys %{$wtf->{$_}}} qw'new diff gone';
                    saybl "T.alk  2222  $uf";
