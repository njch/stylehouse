                        grep{!ref $o->{$_}} keys %$o) {
                        $stick = 1; # unless y.? nonref changed, or wider spacer
                        saybl "J.name unstuck due to ychange: $_:"
                            ." $o->{$_} ne $O->{$_}";
                    }
                }
                # rhythm^
                if ($stick) {
                    A.ico.wtf.same = 0;
                }
                elsif (wtf.same) {
                    T.iM = $iM if $iM;
                    return 0;
                }
                .
                # for the lift, osc should Jiter ne A.J and pd it
                J.A.ljm = [];
                J.A.ljmo = "fr  J.name";
                return 1;
            }
            .
            warn "T.alk other $q";
            if ($q == 8) {
                # until J.most coord attention evaporates
                # as in pulling on a list
            }
            A.ztime >= $q;
        };
    
