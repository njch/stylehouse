j: |
    my $J = ar.J || G.oin ||= Rw Jto sea;
    
Jto: |
     my $J;
     J.id = mkuid;
     J.name = @{delete ar.m};
     J.qq = {};
     J.bb = {};
     J.phy = {};
     J.phl = {};
     
     J.n = sub {
         my $ar;
         ar.J = $J;
         if (ref $_[0] eq 'ARRAY') {
             # is Mn($M,$J,t,y,c,sc)
             ar.M = shift;
             die "wtff" if ar.J ne shift;
         }
         ar.N = [[@_]];
         Rw MyN;
     };
     J.u = sub {
         my $ar;
         ar.J = $J;
         ar.N = [[@_]];
         Rw MyN;
     };
     
     Rw suJ $J;
     
     Rw Jup $J;
     
     $J
Trail: |
sea: |
    
blus: |
    u 
    

