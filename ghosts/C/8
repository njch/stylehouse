 - K: Fscrape #c to drag tthrough pipe
   D: |
     my @fs;
     for my $F (@F) {
         my $f;
         f.K = F.K;
         f.point = F.point;
         if (!@fs) {
             G.doc.Fknow->{F.id} = $F;
         }
         f.id = F.id if G.doc.Fknow->{F.id};
         f.id ||= '?'; # tracable from near id
         push @fs, $f;
     }
     

