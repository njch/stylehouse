   D: |
     my $or = {%$ar};
     or.G = $G->pi;
     or.R = (delete or.R)->pi;
     my $m = wdump($or);
     $m =~ s/^\s+//sgm;
     
     my $ftrain = [map{
        "_.id _.K _.point ar=".join',',sort keys %{_.ar}
        }@F];
     
     my $meta = Rw mJ doc;
     my $int = " W:ar.W.y ar.W.id   " if ar.W;
     say "Emanating $int meta.y";
     say wdump $meta;
     w emana[$meta,$m];
     
