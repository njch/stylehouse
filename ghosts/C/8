 - K: Doc/Fscrape #c to drag tthrough pipe
   D: |
     my @fs;
     my @iF = @F;
     shift @iF for 1..2;
     for my $F (@iF) {
         my $f;
         f.K = F.K;
         f.point = F.point;
         if (!@fs) {
             G.doc.Fknow->{F.id} = $F;
         }
         f.id = F.id if G.doc.Fknow->{F.id};
         f.id ||= '?'; # tracable from near id
         f.intr = {map{$_=>gp(F.intr->{$_})}keys %{F.intr}} if F.intr;
         
         push @fs, $f;
     }
     \@fs
