R_Jz:
 - K: ii/standingtext #c
   D: |
     # find standing waves, columns with something there all the way down
     Rw dock $m $d
 - K: dock #c dock swampdoctor
   D: |
     my $m = ['doc'];
     my $meta = H.G w mJ[$m];
     
     my $or = {%$ar};
     or.G = $G->pi;
     or.R = (delete or.R)->pi;
     my $en = wdump($or);
     $en =~ s/^\s+//sgm;
     
     $m = {};
     m.ftrain = [map{"_.id _.K _.point"}@F];
     m.ar = join',',sort keys %{_.ar};
     m.en = $en;
     H.G w emana[$meta,$m];
     
     # J up zee, store in G.swamp here
     say "Swamp doctor: ".slim 20, $en;
     # spurt swamp zip & cargo
     # someone asks this Hid (sleep mode)
     # availing benignities before and after the fact)

