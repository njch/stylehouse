 - K: ii/standingtext #c
   D: |
     # find standing waves, columns with something there all the way down
     Rw doc $m $d
 - K: doc #c dock swampdoctor
   D: |
     my $m = ['doc'];
     my $meta = H.G w mJ[$m];
     
     my $or = {%$ar};
     or.G = $G->pi;
     or.R = (delete or.R)->pi;
     my $en = wdump($or);
     $en =~ s/^\s+//sgm;
     
     $m = {};
     m.Fscrape = Rw Fscrape;
     my $f = $F[0];
     m.ar = join',',sort keys %{f.ar};
     m.en = $en;
     H.G w emana[$meta,$m];
     
     # J up zee, store in G.swamp here
     # spurt swamp zip & cargo
     # someone asks this Hid (sleep mode)
     # availing benignities before and after the fact)
 - K: Fscrape #c to drag tthrough pipe
   D: |
     my @fs;
     my @iF = @F;
     shift @iF for 1..2;
     for my $F (@iF) {
         my $f;
         f.K = F.K;
         f.point = F.point;
         if (!@fs) {
             G.doc.Fknow->{F.id} = $F;
         }
         f.id = F.id if G.doc.Fknow->{F.id};
         f.id ||= '?'; # tracable from near id
         push @fs, $f;
     }
     \@fs

