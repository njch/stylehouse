 - K: very #c tak tlv into space, ready for lx/suro
   D: |
       my $onl = Rw tlv $J;
       my $phi = $out - $in; # <-0.8->
       my $thi = $phi / @$onl; # or indi
       # thi vs $onl individuality
       my $tin = $in;
       for my $x (@$onl) {
           # go higher and pipe mods to squish space
           x.in = $tin;
           x.out = $tin + $thi;
           $tin = x.out; # next
           
           x.thi = $tot/$phi; # space volve, indi...
           #die "x.j.r   shall x.thi    ($tot $phi)";
           
