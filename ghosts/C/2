 - K: ii/cap #c ha zom
   D: |
       my $vli = x.sc.ori || die;
       
       my $N = [
           ['_', '',         {s=>x.j.l}, {co=>'38f'}],
           ['=', '',         {}, {fs=>12,co=>'000',sha=>'3 3 3 fff'}],
           ['i', 'sur/to:/', {fo=>[$vli,0.4]}],
       ];
       # root ^
       # travel v
       if (x.sc.travely && @{x.sc.travely}) {
           my $s = " ".join(" ",@{x.sc.travely})." ";
           push @$N,
               ['travely', '', {s=>$s, wsp=>1}],
               ['S',       '', {fo=>[x.j.s, 0.2]}],
       }
       
       my $M = [];
       # cend pwth
       Rw MyN $N $M pwth;
       
       Rw ii/capstyle + $M;
       
