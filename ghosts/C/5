 - K: vimcolor #c
   D: |
      my $string = shift;
      my $h = dig($string);
      my $k = "vimcolor-$h";
      H.r.gest->($k, sub {
          use Text::VimColor;
          my $syntax = Text::VimColor->new(
              string => $string,
              filetype => 'perl',
          );
          return $syntax->html;
      });
