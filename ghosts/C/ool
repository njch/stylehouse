 - K: Images
   Gw: 1
   styles: red
     
 - K: Image
   styles: reson
   S:
     styles: red
   u_from:
     spawn:
      - K: i
        styles: red
   
 
 - K: Ooogida
   styles: sativo
dials:
  max_depth: 3
hooks:
  ux_receive: |
    G.ux = $GG;
    my $uW = 0->CsK({K=>'W'}, G.ux);
    uW._Ss = ['x'];
    
  percolate: |
    0->TafuB(Ooogida => {whatcha => "Woogie"});
    0->TafuB(Images => {dir => '/s/public/i'});
    
    # alright so... see styles run around
    # mod styles to mod waves of stuff
  Images:
    flows_D: |
       my @finds = map { 0->fs_find($_) } $dir;
       for my $file (@finds) {
           0->TafuBl(Image => { file => $file });
       }
  uxd: |
    G.ux->w('ux', {W=>$G->W});

