R_surz:
  - K: t___a #c
    babz:
      'maka 4': |
        my @r = w stylekeysort(d=>a.bb);
      
      'maka 4.1': |
        a.o = {}; # by voltage, peel
        for my $r (@r) {
                my ($l, $vo, $or) = split /\s+/, $r; # split style, vectory
                
                saygr "igggy $l $vo > a.cv" && next if $cv && $vo > $cv;
                
                $or = $vo if !defined $or;
                
                my $o = a.o->{$or} ||= []; 
                push @$o, $r;
        }
      'maka 4.2': |
            #thingness <-> appearance
            my $th = {};
            my $ap = {};
            my @int;
            for my $ov (sort keys %{a.o}) {
                 my $o = a.o->{$ov};
                 
                 for my $r (@$o) {
                     my $j = $R->shj($r, a.bb);
                     (j.l, my $style) = split '%', j.t;
                     
                     $style ? do {
                         j.style = $style;
                         push @{$ap->{j.l} ||= []}, $j;
                     } : do {
                          warn "replacemont j.l " if exists $th->{j.l};
                         $th->{j.l} = $j;
                         push @int, j.l;
                     }
                }
            }

