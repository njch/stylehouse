P:
 - GAK: D #c
   InC:
       W:
         Ss: [u, x, X]
         S:
           styles: OxO ab z NZ
       M:
         S:
           styles: OxO
       ux:
         spawn: []
hooks:
  mux: |
    my $D = K G:D;
    my $a = {%$ar};
    my $u = delete a.u;
    my $MR = G.R;
    a.v ||= 7;
    saybl "Gungto Dee mux ..." for 1..5;
    $D w mux[$MR,$a,$u];

