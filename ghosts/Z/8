 - K: on #c
   n_D:
     yJ: |
       J.yl.nojb = 1;
       'on';
     up: | # tak tlv J.on into iterations of upgoing clever
       Rw thy $oJ:J $J:J.Jo defon;
       
       my $vs = Rw tyv $J $ip:ar.ip;
       
       sayyl "ON J.Jo.name";
       my $nc = -1;
       for my $lv (@$vs) {
           $nc = 0 if $nc == -1;
           my $j = lv.j;
           my $coi = Rw pha $J $k=j.r;
           coi.off=100;
           my $lnc = Rw lv_same $lv $j $x:coi $J;
           $nc = 0 if !$lnc;
           if ($lnc) {
               sayyl "LINEHUM SAME";
               #next;
           }
           
           my $sc = {};
           if (!lv.sc) {
               say "INIT STYLO ". lv.j.r ."     ".ki lv.sc;
               if (j.s =~ /^[A-Z0]\/./) {
                   sc.ou = 'file';
                   sc.dir = 'ghosts/';
               }
               # upgrade strings to thoughts
               # drag in
               Rw jamsc $J $j $sc;
           }
           
           coi.end->();
       }
       $nc;
       
       
