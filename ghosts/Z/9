 - K: tks_cs #c tks -> style=>v
   D: |
     my $cs = {};
     for my $k (sort keys %$tks) {
         my $w = $tks->{$k};
         my @wks = sort keys %$w;
         my ($el) = @wks;
         my $v = $w->{$el};
         if (@wks > 1 && 0) {
             warn "ar.r   style $k collaprse ::: ".ki($w)."\n"
             ." went with $el: $v";
         }
         $cs->{$k} = $v;
     }
     $cs
           
