 - K: scitl #c iterate iterations
   D: |
     my @dl = @$styles;
     warn "nodl" if !@dl;
     
     my $i = 0;
     my @om;
     my $flying = 0;
     sub {
         if (@om) {
             my $o = shift @om;
             $flying = 0;
             return @$o;
         }
         while (1) {
             return if !keys %$tks;
             if (!exists $dl[$i]) {
                 $i = 0;
                 if ($flying++ > 2) {
                     my $cs = {};
                     $cs->{$_} = delete $tks->{$_} for keys %$tks;
                     $flying = 0;
                     if (X.FLYING) {
                         return 'FLYING', '', $cs, $tks;
                     }
                     else {
                         sayre $F[2]->pi,
                         $F[0]->pi,
                         "    unnibblable (make FLYING to catch): ".ki 2, $cs;
                         return;
                     }
                 }
                 next;
             }
             my $tk = $dl[$i];
             if (!exists $tks->{$tk}) {
                 $i++;
                 next;
             }
             my $oms = delete $tks->{$tk};
             # delete so if tks come back, we repeat
             for my $com (sort keys %$oms) {
                 my $v = $oms->{$com};
                 push @om, [$tk, $com, $v, $tks];
             }
             $i++;
             if (@om) {
                 my $o = shift @om;
                 $flying = 0;
                 return @$o;
             }
         }
     }
   

   
