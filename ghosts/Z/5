 - K: pi/fol #c
   D: |
     # LOOK FOR KNOWNS, scoped in wormhole
     die if ref $u;
     Jn(name => 0.1, $u);
     
     my @m = map{$H->fixutf8($_);chomp;$_}`/bin/ls -lh '$u'`;
     
     my $tot = shift @m if $m[0] =~ /^total/;
     if ($tot) {
         Jn(D => 0.22, 'D');
         Jn(tot => 0.22, $tot);
     }
     my @o;
     my $i = 0;
     for my $n (@m) {
         my $d;
         $n =~ /^(\S+)\s+(\S+)\s+(\S+)\s+(\S+)\s+(\S+)\s+(\S+\s+\S+\s+\S+)\s+(.+)$/;
         d.t = $7;
         d.y.i = $i++;
         d.c.s = "$u/$7";
         d.c.pi = "fol";
         d.sc.size = $5;
         d.sc.time = $6;
         push @o, $d;
     }
     
     die $RADIAN;
     # ire culture suggestions
     my $M = [@o];
     Rw vary $J $M;
         push @{o.r}, $r;
         if (@{o.r} >= $thi) {
             o.more = @r - @{o.r};
             last;
         }
     }
