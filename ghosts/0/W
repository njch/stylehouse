 
0:
 W:
  - K: head #c
    D: |
      use base 'Wormhole';
  - K: new #c
    D: |
      W.G = W.A.u.i;
      W.name = "W.G.name";
      W.n = 0;
      $W->wormfile_load(shift);
      
  - K: uhigh #c
    D: |
      my $n = $W->{n};
      my $u = scalar(@{$W->{script}});
      ($u ne $n ? $n : '')."x$u"; # acquire north, get high
      
  - K: pi #c
    D: |
      "W W.name ".$W->uhigh;

