  - K: fs_glob #c
    D: |
        my (@globs) = @_;
        my @list;
        for my $glob (@globs) {
            push @list, grep { defined }
                grep { $H->fixutf8($_) || 1 }
                grep { -f $_ } glob $glob;
        }
        return @list;
  - K: fs_find #c
    D: |
        my (@dirs) = @_;
        my @list;
        File::Find::find(sub {
            my $na = $File::Find::name;
            $H->fixutf8($na);
            return unless -f $na;

            push @list, $na;
        }, @dirs);
        return @list;
