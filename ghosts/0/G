          an.e -= $ohms;
          
          my $rk = "$k an.e";
          $c->{$rk} = $v;
          
          say join("", ("  ") x scalar(@{a.ds}))
              ." a.e - $ohms  $k\t an.e \t\t ".gpty($v);
                    
