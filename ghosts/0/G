  - K: B_same #c
    D: |
        my ($u, $i) = @_;
        
        return 1 if exists u.B._ && exists i.B._ && u.B._ eq i.B._;
        
        my @ks = keys %{u.B};
        for my $k (@ks) {
            u.B->{$k} eq i.B->{$k} || return 0;
            next if $k eq "Codon";
        }
        unless (keys %{u.B}) {
            Say " u has no B, mind  you...".ki($u);
        }
        return 1;
