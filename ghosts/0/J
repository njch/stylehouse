      
Ds:
  - K: new #c
    D: |
      J.R = J.A->fiu('R');
      J.G = J.A->fiu('G');
      
  - K: s #c
    D: |
      J.R = J.A.u.i;
      if (ref J.R eq "G" && 0) {
          J.R = J.R.G.R;
          J.G = J.R.G;
      }
      #J.R->dfrom($J, {B=>shift});
      
  - K: pi #c
    D: |
      "J(".J.R->pi;

