  - K: suets
    D: |
      my ($i, $s) = @_;
      my @s = @{s.s};
      my $end = pop @s if exists s.e;
      my $last;
      while (1) {
            my $ac = shift @s || do {exists s.e || last; $last=1; $end};
          $ac =~ /^(\W)(\w+)$/ || die;
          if (!$last) { # TODO know about insto hash or array...
              $i = $i->{$2} ||= {} if $1 eq "{";
              $i = $i->[$2] ||= {} if $1 eq "[";
          }
          else {
              $i->{$2} = s.e if $1 eq "{";
              $i->[$2] = s.e if $1 eq "[";
              last;
          }
      }
  - K: snapple
    D: |
      my $k = shift;
      ($k, my $v) = $k =~ /^(\S+)(?: (.+))?$/;
      my $a = {k => $k, v => $v};
      my @s;
      while ($k =~ m/(\W\w+)/sg) {
            push @s, $1;
      }
      a.s = \@s;
      $a
      

