                   }
               }
               
               # GP ness ^ v leaky
               my $bcv = 0.4;
               $bcv = 0.1 if r.cv < 0.2;
                  
               J2.fro->("gp\t"."0.6" => r.s,
                 {cv=>$bcv, makaz=>{ # hooky
                    "/%css#jit 0.9" => 'position:absolute;left:1em;max-width:14em;overflow:hidden;white-space:nowrap',
                 }}, # ^ v in out sameish
               );
           },
                 (r.path ? ('%path' => r.path) : ()),
           );
            #J.bz->("$r\t"."0.$cv" => ''.$v, '%css'=>'white-space:pre');
             # has l cursor stashed as a 1shj
