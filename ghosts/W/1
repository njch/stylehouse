     to: | #c into
       my $to = shift @$m;
       my $r;
       for ($to) {
           r.t = [] when '/';
           r.i = $G when 'G';
           die         when 'feed'; # look at cont moves?
           r.i = Rw Jung
                       when 'Jung';
           r.i = Rw onJ o2x theself
                       when 'self';
           
           default {
               die "no yyyyyyyyyyyy to $to   on ".$J->pi;
           }
       }
       Rw inness $J $r;
