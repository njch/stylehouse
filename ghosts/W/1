               push @r, { r=>$i, path=>"[".$i++, s=>$ui, cv=>$cv};
           }
       }
       else {
           my $r = {map{$_=>1} keys %$u};
           
           $spcv = 0.3 if u.id;

           # bunch of keys... ^ interpret, compress knowledge
           # TODO ^make itv like two indexes, polars
           # any sorta layout knowhow...
       
