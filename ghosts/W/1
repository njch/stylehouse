       ];
       my ($vly) = grep{_.style eq 'travely'} @{rr.sjs};
       if ($vly && @{vly.s}) {
           push @$M,
               ['travely', '', {s=>join("  ",@{vly.s}), wsp=>1}],
               ['S', '', {fo=>[rr.j.s, 0.2]}],
       }   
       # ^ all that within v
       my $lev = sub {
           my $J2 = shift;
           Rw mo/pwth $J:J2 $M 0.1-0.3;
       };
       ['cap','', {lev=>$lev}]
     rr_bod: | #c boodle am
       my $in = rr.in;
       my $out = rr.out;
       my $phi = ($out - $in) * rr.thi; # normal lines per 0.1
       my $thi = $phi * 2; # oundo big window, bod small
       
       my $j = rr.j;
       my $sjs = rr.sjs;
       my $u = j.s;
       say "bod $in <--> $out  $thi     j.r";
       
       # how to what = @{rr.sjs}
       my $o = Rw oundo $thi $u;
       
       # between here and oundo is:
       # the art of extracting segments like pi/HASH etc
