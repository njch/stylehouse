              ['travels', '', {s=>join("  ",@{on.t||[]}), wsp=>1}],
              ['fou', '', {fo=>[$u, 0.2]}],
                  if $u ne on.i;
                  
           Rw mo/pwth $J:J2 $M 0.1-0.3;
       });
