     upeng: | #c the surrest
       my @things = Rw tvy $J:J.on;
       my $tot = 32;
       my $in = 0.1;
       my $out = 0.9;
       my $phi = $out - $in; # <-0.8->
       my $tin = $in;
       my $thi = $phi / @things; # or indi
       my @re;
       for (@things) {
           my ($j, @sjs) = @$_;
           say "U: ".$J->pi."   ".gp(j.s); 
           say "   * _.r          => ".gp(_.s) for @sjs;
           my $outt = $tin + $thi; # r indi, of sjs
           my $rr = {
               in => $tin,
               out => $outt,
               thi => $tot/$phi, # space volve, indi...
               sjs => \@sjs,
               j => $j,
           };
           say "rr.j.r    = rr.in    -> rr.out";
           push @re, $rr;
           $tin = $outt;
       }
       # each branching yet here...
       for my $rr (@re) {
           my $M = [
               Rw sur/rr_cap $rr,
               Rw sur/rr_bod $rr,
           ];
           my $in = rr.in;
           my $out = rr.out;
           Rw mo/pwth $J $M $in $out;
       }
       sayre "==\n== delt ".F_delta."\n==";
     rr_cap: | #c zom
       my ($vli) = grep{_.style eq 'traveli'} @{rr.sjs};
       my $M = [
           ['_', '', {s=>rr.j.l}, {fs=>18, co=>'38f'}],
