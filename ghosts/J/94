R_Jz:
 - K: stylesheets #c
   babz:
     'recoded_init 5.543': |
       Rw stylesheets;
   D: |
     my $tw = Rw huck */stylesheet;
     
     while (my($t,$w) = each %$tw) {
         my $as = {K=>'style', B=>{am=>$t}};
        my $u = w G.R fuu(u=>$as);
        my $was = u.sheet;
        u.sheet = join "\n",@$w;

        w mux[$u] if u.sheet ne $was;
     } 

