 - K: mod #c make pool, synth space, want arches
   D: |
     if (ref $d eq 'ARRAY') {
         my $m = $d;
         $d = {};
         (d.t, d.thi, d.s, d.c, d.css, my @etc) = @$m;
         %$d = (%$d, @etc) if @etc;
     }
     if (!ref d.c) {
         d.css = d.c; # m
         delete d.c;
     }
     d.css ||= [];
     d.css = ['%css', d.css] if !ref d.css;
     
     d.in ||= 1;
     d.out ||= d.thi;
     
