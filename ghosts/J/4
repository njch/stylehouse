        if (my $bs = y.bal) {
            y.thi = @$bs;
        }
     }
     y.in ||= 0.1; 
     y.out ||= 0.9; # up to
     y.thi ||= 1;
     y.space = y.out - y.in; # could spiral much space away from spine
     die unless y.space > 0;
     y.aspace = y.space / y.thi;
     # twist, lay into segment
     my @is;
     my $csv = y.in;
     for (1..y.thi) {
        push @is, [$csv, $_ / y.thi];
        $csv += y.aspace;
     }
