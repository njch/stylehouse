 - K: expowsc #c espouse ton carbon product
   D: |
     my $seg = ar.m->[0] if ar.m;
     my @css;
     die if !ref $sc; # parse strings too
     # invent the stevecraig format, playing music
     my $X = G.drop.expowsc ||= {};
     X.sha = sub { # look familiar...
        my @m = split m/\s/, shift;
        $m[-1] = X.cole->($m[-1]);
        sprintf 'text-shadow:%dpx %dpx %dpx %s', @m;
     };
     X.cole = sub {
        my $v = shift;
        my $h;
        (h.r, h.g, h.b, h.la) = ($1, $2, $3, $4)
                  if $v =~ /^(\w)(\w)(\w)(\w)?$/ || die;
        
        if (defined h.la) {
           # do an rgba(255,255,255,1)
           for my $k (qw'r g b al') {
               my $e = $h->{$k};
               $e = "$e$e" if length $e == 1; # out of 255
               $h->{$k} = hex $_;
           }
           h.al = h.al / 255; # out of 1
           return "rgba(h.r,h.g,h.b,h.la)";
        }
        else {
           return sprintf "#%s%s%s", h.r, h.g, h.b;
        }
     };
     
     X.co = sub {
        "color:".X.cole->(shift);
     };
     
     X.fs = sub {
        my $fs = shift;
        $fs *= 9;
        'font-size:'.int($fs).'%';
     };
     
     X.ws = sub {
        my $wtf = shift;
        my @wtf;
        push @wtf, 'white-space:pre' if $wtf =~ /p/;
        join ';', @wtf;
     };
     
     X.blr = sub {
        '-webkit-filter:blur('.shift.'px)';
     };
     X.ml = sub {
        'margin-left:'.shift.'em';
     };
     
     my $tw;
     for my $k (sort keys %$sc) {
         my $v = $sc->{$k};
         my $com = ref $v eq 'HASH' ? $k : '';
         my $t = $com ? $v : {$k=>$v};
         for my $tk (sort keys %$t) {
             my $tv = $t->{$tk};
             my $W = $tw->{$com} ||= [];
             my $s = $X->{$tk} ? $X->{$tk}->($tv) : die "invent $tk  $tv";
             say " cs #$com# $k    = $tk => $tv         $s";
             push @$W, $s;
         }
     }
     for my $com (sort keys %$tw) {
         my $W = $tw->{$com};
         $com = join'-', grep{$_} $seg, $com; 
         my $sty = '%css';
         $sty .= '#'.$com if $com;
         push @css, $sty => join ';', @$W;
     }
         sayre "CSS  @css";
     @css;
