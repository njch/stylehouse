   D: |
     my $peel = sub { # simple hash from string
        my $s = shift;
        $s =~ s/^{//;
        $s =~ s/}$//;
        my @t = split ',', $s;
        my $t = {};
        for (@t) {
            my ($k, $v) = split ':', $_;
            $t->{$k} = $v;
        }
        return $t;
     };
     map {
         my $n;
         if (!ref $_) {
             die "string: $_";
         }
         elsif (ref $_ eq 'ARRAY') {
             # to four
             if (@$_ eq 1) {
                 my ($i) = @$_;
                  if (!ref $i) {
                     my $ci = $i;
                     for (qw,t y c sc,) {
                         if ($ci =~ s/^(\S+)\s*//) {
                             my $s = $1;
                             $s = $peel->($s) if $s =~ /^{/;
                             $n->{$_} = $s;
                         }
                     }
                 }
                 else {die $i}
             }
             else {die'@>2'}
         }
         elsif (ref $_ eq 'HASH') {
             if (_.MZ) {
                 
             }
             else {
             die 'hash: ';
             }
         }
         else { die "mmtype:$_" }
         $n
     } @$m
