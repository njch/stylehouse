 - K: mm #c mthem
   D: |
     my $peel = sub { # simple hash from string
        my $s = shift;
        $s =~ s/^{//;
        $s =~ s/}$// || die "unpealable $s";
        my @t = split ',', $s;
        my $t = {};
        for (@t) {
            my ($k, $v) = split ':', $_;
            $t->{$k} = $v;
        }
        return $t;
     };
     my @mm=();
     my $fl={};
     for my $s (@$m) {
         my $n = {};
         if (!ref $s) {
             die "string: $s";
         }
         elsif (ref $s eq 'ARRAY') {
             # to four
             if (@$s eq 1) {
                 my ($i) = @$s;
                 if (!ref $i) {
                     my $ci = $i;
                     for (qw,t y c sc,) {
                         if ($ci =~ s/^(\S+)\s*//) {
                             my $v = $1;
                             $v = $peel->($v) if $v =~ /^{/;
                             $n->{$_} = $v;
                         }
                     }
                 }
                 else {die $i}
             }
             else {die'@>2'}
         }
         elsif (ref $s eq 'HASH') {
             if (s.MZ) {
                 for (split /\s+/, s.MZ) {
                     /^(.+?)(?::(.+))?$/ || die;
                     $fl->{$1} = $2 || 1;
                     sayre "MZfilter: $1 and $2";
                 }
                 next;
             }
             else {
             die 'hash: ';
             }
         }
         else { die "mmtype:$s" }
         for my $k (keys %$fl) {
             my $v = $fl->{$k};
             die if $v ne '1';
             Rw ym/$k $n;
         }
         push @mm, $n;
     }
     @mm
