     } @$oM ];
     say "G.jumb.yos:::: om/bal: x".@$oM."  t:d.t    y:".ki(d.y)."        c:".ki(d.c)."       sc:".ki(d.sc) if ar.V;
     #say "G.jumb.yos ^^ ".ki $ar;
