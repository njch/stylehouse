         my $c = d.c;
         $c = {} if !ref $c;
         d.c = sub {
             my ($i)  = @_;
             my $v = $i / (999 - d.in);
             # 3.2
             my @gl;
             # 3.3
             # 3.4
             # 3.5
             # add style tio 
             # 3.6
             @gl = d.s if !@gl;
             d.s = join'', @gl;
         };
     }
     
     my @is = map { int($_ * (999/d.thi)) } d.in..d.out;
     
     my $dcss = d.css;
     for my $i (@is){ # THE TIR
         d.css = [@{$dcss}];
         d.s = d.c ? d.c->($i, $d) : d.s;
         
