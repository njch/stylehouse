    - K: stylestylekeysort #c comp-ression 
      D: |
        my @r = sort keys %$d;
        my $by = { map {
            my @w = split "\t", $_;
            warn " Oi '$_' not stylestyle or not using tabs proply" if @w != 3;
            my $s = join"\t", $w[1], $w[0], $w[2]; # scv, st, cv
            $s => $_;
        } @r };
        @r = map { $by->{$_} } sort keys %$by;
        
        @r

