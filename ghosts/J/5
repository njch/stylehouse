             J.bz->("$_%css#uff\t".j.cv =>
                 #'border-bottom: 2px solid white'
                 'opacity:0.3;'
                 )
             when /^stuff(\d+)/;
             
             J.bz->("$_%css#wa\t".j.cv =>
                'border-bottom: 3px solid white'
                 )
             when 'name';
         }
      }
 - K: mod #c make pool
   D: |
     my $in = 1;
     
     if (ar.over) {
         $in += ar.over;
        $thi += ar.over;
     }
     
     J.bz->("$t$_\t0.".sprintf('%03d',$_) =>
         (ar.c ? ar.c->($_) : ar.s),
        @{ar.css}
        )
        for
        map {  int($_ * (999/$thi)) }
        $in..$thi


