R_Jz:




 - K: up #c
   D: |
     Rw suck + */up;
 - K: around #c
   D: |
     inter({Jname=>ar.J.name}) if exists ar.J;
     my $tw = Rw huck + */around;
     
     if (!$tw) {
        Rw up +;
        Rw out +;
     }
     
     saygr "around J.name ".F_delta();
     # try return stuff? pretty aroundo
     @{(values %$tw)[0]} if $tw
 - K: out #c
   D: |
     Rw suck + */out;

