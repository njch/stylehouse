name: Ϡ
etc: intoraction...
include: W/tractor
tractors:
  - K: 'Ϡt'
    max_populi: 2
    arr_if: |
      $name && $name ne "0" && $name ne "1" || die;
    B_: |
      $name
    print: |
      $S->{B}->{_}      
hooks:
  V:
    ux:
      styles: cardboard ux
  styles:
    cardboard: |
      qw{
         position:absolute
         color:#3bc 
      }, 'border:2px solid #003'
      
    ux: |
      qw{ width:80% height:80% top:10% left:10%    font-size:10pt
      }
    xu: |
      qw'left:90% background:#513'
  GG:
    C/odon: 1
    T/ux: 1
  any_child_recoded_init: |
    w wux;
  recoded_init: |
    w wux;
  ux: |
    my $g = G(odon)
    ||
    return Say "No Codon?";
    sayre "REE E E EEE" for 1..4;
    $g->RW;
    $g->w('percolate');
    
    my $gux = $G->Gf('ux$');
    gux.view = G.V.ux;
    g.ux = $gux;
    $gux->RW;
    w wux;
    saybl "ux ".$G->F_delta();
  wux: |
    G(odon) ->w('uxd');

