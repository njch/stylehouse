






name: kasplat
hooks:
  any_init: |
    #die " $G->{load_ways_count} ";
    #unless (Gf crawl ) {
        G TT ->G("T/crawl");
    
    #die 
    G crawl ->T("hello");
    say "wormhole now ".G crawl ->W->describe_size();
  throwlog: |
    my $s = $G->stackway(@$thing);
    
    $G->timer(0.1, sub {
    $L->w(somewhere => {thing => $s});
    $G->timer(3, sub {
    $L->w(somewhere => {thing => $G->stackway($s)})
    }); 
    }); 
    
  event: |
    #w throwlog(Event => $e);
    if (eval { $ar->{e}->[1]->{C} }) {
        my $id = $ar->{e}->[1]->{id};
        $H->JS("\$('#$id').empty();");
    }
    if (eval { $ar->{e}->[1]->{S} }) {
        my $id = $ar->{e}->[1]->{id};
        $H->JS("\$('#mess').toggleClass('inface');");
    }
    

