my ($from,$to) = ('s','v');

my $date = `ssh $from date`;
my $dath = `ssh $to date`;
chomp $date;
chomp $dath;

say "From $from: $date";
saybl "Over $to:  $dath";
`ssh $to "sudo date --set '$date'"`;
sayyl "Done $to now: ".`ssh $to date`;

return;
