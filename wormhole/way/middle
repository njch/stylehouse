my $E = Rw CsE $C;
A.V&E &&
say "midd: C.t has E.t ".(E.c.us && "(us)");
# us, ids are brain tickets, tractorbeam does Elis first time
my $us = E.c.us;
if (J.ev.tr && E.c.trJ ne $J
    || !$us
    || us.ids && us.ids->[E.c.us.idi - 1] eq J.id
    || us.p && do { my $tcv = us.p->[E.c.us.p_i - 1]; $tcv && $tcv->[0] eq J.name }
    ) {
    Rw Elis + $E;
}
else {
    Rw zyeuter + $E;
}
