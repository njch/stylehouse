n 'allof'=>411,<<''
    if (C.c.of) {
        !C.t || die "ofall".ki$C;
        A.J.A.vse.of = C.c.of;
        return T.not = 1;
    }
    C.sc.code ||= do {
        my $cv = C.y.cv || 1;
        $cv =~ s/^0\.//;
        my $of = A.J.A.vse.of;
        $of ||= do {
            warn "Using Ct code=C.t $cv   for ".ki $C;
            C.t
        };
        "$of $cv"
    };
