map{delete $J.>$_ }qw'olp ghut' if J.olp++ > 10;

J.ghut ||= [ G&qh,<<'' ];
    select file, t, ts from gho order by file, t

J.seg ||= 16;
J.gset ||= [shuffle grep{_.t =~ /^[A-Z]/}@{J.ghut}];
J.yset ||= [@{J.gset}[1..J.seg]];
J.seg++;
my $l = @{J.yset};
J.yset->[int rand($l)] = J.gset->[J.seg];

m _.t=>-yamco for @{J.yset}
