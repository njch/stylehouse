my ($t,$isle) = reverse @{ar.m};
($t,$isle) = reverse split '/', $t, 1 if $t && !$isle;
$t && $isle || die " Arg m proper: $t+$isle";

my ($A) = 
n $t=>W=>{isle=>$isle},'%noXup'
my $ch = A.nj;
my $X = $ch && ch.El.Duv.rv.X
    || A.M->[0]->{sc}->{X}
    || die "NO X on ch.name for $isle $t";
my $N = [];
Rw sumX $X $N;
($X,$N)
