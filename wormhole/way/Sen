y.in = 0.02;
y.out = 0.5;
y.at.sca = 0.3;
$A = 
s:
    t

if (!-t STDIN) {
    `cd proc/; ln -s $$\.out G.t-G.cv`;
}
G.Jsem = sub {
    my $em = shift;
    my $db = G&db;
    $em = {lines=>[$em]} if !ref $em;
    map{$db->notify('Js',$_)} map{decode_utf8($_)} @{em.lines};
};

Rw End
