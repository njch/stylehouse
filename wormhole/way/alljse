n 'allarejse=s'=>2,{s=><<''},{code=>'In 419'}
    return T.not = 1 for
        map { A.J.A.vse->{$_} = C.c->{$_} }
        grep {exists C.c->{$_}} qw'of nk';
    my $av = A.J.A.vse;
    C.sc.of ||= av.of||'om';
    unless (C.sc.any) {
        C.sc.nk ||= av.nk||'sc';
        C.sc.gk ||= C.t;
    }
    my $cv = C.y.cv || 1;
    $cv =~ s/^0\.//;
    C.t = C.sc.of."_".C.t."_".$cv;
