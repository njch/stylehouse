my $J = G.oin || return sayre "Cannot G-Byst-ander yet";
my $sc = ar.sc || ar.N || ar.M || die "n lost";
if (ref $sc->[0] && @$sc > 1) {
    return join "\n", map {
        my $sc = [$_];
        my $s = Rw ZNews $sc;
        $s =~ s/^! /oo/s;
        $s;
    } @$sc;
}
elsif (!ref $sc->[0] && @$sc) {
    return warn "Raw ".wdump $sc;
}
my $N = [@$sc];
@$N = Rw humms $J $N wongui _;

my $v = {talks=>[],indcar=>'from'};
Rw humms $J $N talky $v;
join "\n", @{v.talks};
