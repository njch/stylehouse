my $n = Rw Dwive 3;

if ($n == 0) {
    n qqreps =>W=>'%test:1'

    #n DwightSmall =>W=>'%test:1'

    n CarSmall =>W=>'%test:1'

    n EwokSmall =>W=>'%test:1'
}
elsif ($n == 1) {
    n EwokSmall =>W=>'%test:2'
}
elsif ($n == 2) {
    n EwokSmall =>W=>'%test:3'
}
else {
    ok(0 => "Run too many times! ($n)");
}
