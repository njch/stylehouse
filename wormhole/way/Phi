#n Spirals
n \'of:Mo'

cola 5:
    my $start = $cola || 1;
    $start *= 10;

    my @ay = ($1, split'',$1) if C.y.pcv =~ /\.(\d+)/;
    C.sc.hs = join' ', map {
        my $o = shift @ay;
        $o += 2;
        $o *= $start;
        $o /= 100 until $o < 100;
        int $o;
    } 1..3;
    C.sc.sha = '3 4 8 248';
