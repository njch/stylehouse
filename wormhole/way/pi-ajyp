my $W = C.c.W;

my $Y = W.El->{$s} || return
    n Not=>''=>"no El $s on W.name"

my $rv = Y.rv || return
    n Not=>''=> "nmorv J.name W.name $s"

rv.i && rv.c && rv.sc || return
    n Not=>''=> "norsc ".wdump[sort keys %$rv];


n lisn=>''=>{s=>"rv.i"}

Atime(2)||return;

Mn(c=>''=>{pi=>''=>s=>'c>'.@{rv.c}});

Mn(sc=>''=>{pi=>''=>s=>'sc>'.@{rv.sc}});
