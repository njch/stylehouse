-e $u || return 
Jn('not',0.1,'!!!!'.$u,'%hs:9385');

Jn(lifen => 0.09 => 'l', '%hs:3884,fs:11');
my $si = -s $u;
#Jn(si => 0.1 => $si, '%hs:9584,fs:7');

J.most.filey = $u;  

if ($si > 8000) {
    Jn(more => 0.3, '@', '%mu:r');
}
else {
    my @m = read_file $u;
    Mn(C.t=>'',{pi=>'liel',s=>$_}) for @m;
}
